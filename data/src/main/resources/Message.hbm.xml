<?xml version="1.0" encoding="UTF-8"?>

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="data.Message" table="messages">
        <id column="MessageID" name="messsageID" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property column="Text" name="text" type="java.lang.String"/>
        <property column="Date" name="date" type="java.util.Date"/>
        <property column="UserID" name="userID" type="java.lang.Integer"/>
        <property column="ChatID" name="chatID" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>